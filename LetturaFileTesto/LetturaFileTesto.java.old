            BufferedReader br = new BufferedReader(fr);
            String riga;
            riga = br.readLine();
            while (riga != null) {
                String dati = riga.split(",");
                System.out.println(riga);
                riga = br.readLine();
            }
            br.close();
            fr.close();            
        }
        catch (IOException error) {
